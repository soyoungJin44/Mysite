use web_db;

create table person (
	no			integer			AUTO_INCREMENT PRIMARY KEY
	,id         varchar(20)		UNIQUE not null
    ,password   varchar(20)		not null
    ,name   	varchar(20)
    ,gender		varchar(10)
    );
    
insert into person
values(null, 'wls', 'pass', '진', '여');

